<!DOCTYPE html>
<html>
<head>
	<title>DB -> GRAPHQL</title>
	<meta charset="UTF-8">
	<style type="text/css">
		h4 {
			margin: 1rem;
			padding: 1rem;
			border: 1px black solid;
		}
	</style>
</head>
<body>
	<h3>Upload complete</h3>
	<div><a href="/graphql">browse data via graphql</a></div>
	<div><a href="/">return home</a></div>

	<h4>Users</h4>
	<table>
		<tr>
			<th>id</th>
			<th>firstname</th>
			<th>lastname</th>
			<th>email</th>
			<th>roledesc</th>
			<th>team</th>
		</tr>
		<%users.forEach(user => {%>
		<tr>
			<td><%=user.id%></td>
			<td><%=user.firstname%></td>
			<td><%=user.lastname%></td>
			<td><%=user.email%></td>
			<td><%=user.roledesc%></td>
			<td><%=user.team%></td>
		</tr>
		<%});%>
	</table>

	<h4>Teams</h4>
	<table>
		<tr>
			<th>id</th>
			<th>team</th>
			<th>users</th>
		</tr>
		<%teams.forEach(team => {%>
		<tr>
			<td><%=team.id%></td>
			<td><%=team.team%></td>
			<td>
				<ul>
			<%for (let user of team.users){%>
				<li><%=JSON.stringify(user)%></li>
			<%};%>
				</ul>
			</td>
		</tr>
		<%});%>
	</table>
</body>
</html>